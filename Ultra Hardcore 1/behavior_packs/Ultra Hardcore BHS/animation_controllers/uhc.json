{
	"format_version": "1.10.0",
	"animation_controllers": {
	  "controller.animation.uhc": {
		"initial_state": "alive",
		"states": {
		  "alive": {
			"transitions": [{ "dead": "!query.is_alive" }]
		  },
		  "dead": {
			"transitions": [{ "alive": "query.is_alive" }],
			"on_entry": [
			  "/function sets/player",
			  "/scoreboard players reset @s gm",
			  "/op @s"
			]
		  }
		}
	  }
	}
  }
  